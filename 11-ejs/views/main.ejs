<h1>Vista de productos</h1>


<div class="container">
    <% if (emptyList) { %>
        <div class="alert alert-warning" role="alert">
            La lista est√° vacia
        </div>
        <%}else{%>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">Title</th>
                        <th scope="col">Price</th>
                        <th scope="col">thumbnail</th>
                    </tr>
                </thead>
                <tbody>
                    <% items.forEach(function (item) { %>
                        <tr>
                            <th>
                                <%= item.title %>
                            </th>
                            <td>$<%= item.price %>
                            </td>
                            <td><img width=50 src=<%=item.thumbnail %> alt=""></td>
                        </tr>

                        <% }); %>
                </tbody>
            </table>
            <%} %>
</div>